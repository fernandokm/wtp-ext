{
  "version": "0.1",
  "manifest_version": 2,
  "name": "...",
  "description": "...",
  "background": {
    "scripts": ["/src/background.js"]
  },
  "protocol_handlers": [
    {
      "protocol": "dweb",
      "name": "Distributed Web Protocol",
      "uriTemplate": "/views/load-magnet.html#!%s"
    }
  ],
  "web_accessible_resources": ["*"],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "<all_urls>"
  ]
}
